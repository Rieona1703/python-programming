import re
password = input("Enter a password: ")
is_valid = lambda p: (
    len(p) >= 8 and
    re.search(r"[A-Z]", p) and
    re.search(r"[a-z]", p) and
    re.search(r"[0-9]", p) and
    re.search(r"[!@#$%^&*(),.?\":{}|<>]", p)
)

print("Valid password" if is_valid(password) else "Invalid password")
